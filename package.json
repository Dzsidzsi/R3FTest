{
  "name": "@dzsidzsi/r3ftest",
  "version": "0.0.3",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "description": "React Three Fiber Test",
  "homepage": "https://github.com/Dzsidzsi/R3FTest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dzsidzsi/R3FTest.git"
  },
  "types": "./build/types/index.d.ts",
  "declaration": true,
  "declarationDir": "build/types",
  "emitDeclarationOnly": true,
  "style": "build/styles.css",
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/index.es.js",
      "require": "./build/index.umd.js"
    },
    "./styles.css": {
      "require": "./build/styles.css",
      "default": "./build/styles.css"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@react-three/drei": "^10.2.0",
    "@react-three/fiber": "^9.1.2",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "three": "^0.177.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.177.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
